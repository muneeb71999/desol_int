{
  "name": "ga-crawler",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "ts-node ./src/app.ts",
    "build": "tsc && tsc-alias",
    "dev": "nodemon --exec ts-node -r  tsconfig-paths/register ./src/app.ts",
    "debug": "ts-node-dev --inspect-brk -r  tsconfig-paths/register ./src/app.ts",
    "crawler": "npx ts-node -r  tsconfig-paths/register ./src/schedulers/crawlers/website.crawler.ts",
    "queue": "npx ts-node -r  tsconfig-paths/register ./src/queue.ts",
    "worker:pages": "npx ts-node -r  tsconfig-paths/register ./src/workers/website.worker.ts",
    "worker:formatter": "npx ts-node -r  tsconfig-paths/register ./src/workers/formatter.worker.ts",
    "worker:server:pages": "node ./build/workers/website.worker.js",
    "worker:server:formatter": "node ./build/workers/formatter.worker.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.17.1",
    "@langchain/community": "^0.2.32",
    "@langchain/openai": "^0.2.10",
    "@types/bcrypt": "^5.0.2",
    "@types/cheerio": "^0.22.35",
    "@types/node": "^20.10.4",
    "@types/node-cron": "^3.0.11",
    "axios": "^1.6.3",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "bull": "^4.12.0",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.3",
    "express-validator": "^7.0.1",
    "extract-domain": "^5.0.2",
    "hnswlib-node": "^3.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^8.0.3",
    "node-cron": "^3.0.3",
    "openai": "^4.58.2",
    "path-to-regexp": "^6.2.1",
    "pino": "^8.17.2",
    "slugify": "^1.6.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "underscore": "^1.13.6",
    "yup": "^1.3.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/bull": "^4.10.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "husky": "^8.0.0",
    "ncp": "^2.0.0",
    "nodemon": "^3.0.2",
    "prettier": "3.2.5",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0"
  }
}
